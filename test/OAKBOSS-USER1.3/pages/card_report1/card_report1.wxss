.card_report_body::before{
  content: "";
  display: table;
}
.card_report_body{
  background: #eee;
height: 1209rpx;
}
.back_right{
  width: 9rpx;
  height: 17rpx;
}
.report_info{
  width: 702rpx;
  height: 487rpx;
  margin: 0 auto;
  margin-top: 19rpx;
}
.report_info>view{
  height: 92rpx;
  width: 702rpx;
  background: #fff;
  border-bottom: 1rpx solid #eee;
  display: flex;
  justify-content: space-between;
}
.report_info>view>text{
  line-height: 92rpx;
  font-size: 24rpx;
}
.report_info>view>text:nth-child(1){
  margin-left: 33rpx;
}
.report_info>view>text:nth-child(2){
  margin-right: 29rpx;
  color: rgba(0, 0, 0, 0.45);
}
.report_info>view:first-child{
  border-radius: 20rpx 20rpx 0 0;
}
.report_info>view:last-child{
  border-radius: 0 0 20rpx 20rpx;
}
.warm_prompt{
  width: 626rpx;
  height: 165rpx;
  margin: 0 auto;
  margin-top: -5rpx;
font-size: 24rpx;
  color:rgba(0, 0, 0, 0.45);
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
}
.warm_prompt>text:last-child{
  align-self:flex-start;
}
.btn1{
  width: 703rpx;
  height: 104rpx;
  font-size: 36rpx;
  color: #fff;
  line-height: 104rpx;
  text-align: center;
  margin: 0 auto;
  margin-top: 50rpx;
background: linear-gradient(90deg, #4599EE, #3A6CD6);
box-shadow: 0px 0px 16rpx 0px rgba(107, 130, 153, 0.25);
border-radius: 50rpx;
}
/*使屏幕变暗  */
.commodity_screen {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.2;
  overflow: hidden;
  z-index: 1000;
  color: #fff;
}
/*对话框 */
.commodity_attr_box {
  height: 376rpx;
  width: 100%;
  overflow: hidden;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 2000;
  background: #fff;
  padding-top: 20rpx;
  border-radius: 20rpx 20rpx 0 0;
  padding: 38rpx 23rpx;
  box-sizing: border-box;
}
.radio_item{
  width: 100%;
  height: 96rpx;
  border-bottom: 1rpx solid #eee;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.radio_function{
  margin-top: 10rpx;
}
.report_title>text{
  font-size: 36rpx;
  margin-left: 268rpx;
}
.left_back{
  width: 14rpx;
  height: 26rpx;
  margin-left: 10rpx;
}